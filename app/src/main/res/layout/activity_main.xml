<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="iel.tzy.MainActivity"
    android:orientation="vertical"
    >

    <iel.tzy.watcher.TEditText
        android:id="@+id/edit_text"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:hint="请输入文本"
        android:textSize="18sp" />

    <CheckBox
        android:id="@+id/setComposingText"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="setComposingText" />

    <CheckBox
        android:id="@+id/setComposingRegion"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="setComposingRegion" />

    <CheckBox
        android:id="@+id/finishComposingText"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="finishComposingText" />

    <CheckBox
        android:id="@+id/getTextBeforeCursor"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="getTextBeforeCursor" />

    <CheckBox
        android:id="@+id/getTextAfterCursor"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="getTextAfterCursor" />

    <CheckBox
        android:id="@+id/getExtractedText"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="getExtractedText" />

    <CheckBox
        android:id="@+id/getSelectedText"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="getSelectedText" />

    <CheckBox
        android:id="@+id/setSelection"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="setSelection" />

    <CheckBox
        android:id="@+id/deleteSurroundingText"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="deleteSurroundingText" />

    <CheckBox
        android:id="@+id/commitText"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="commitText" />

    <!--  <TextView
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:text="Hello World!"
          app:layout_constraintBottom_toBottomOf="parent"
          app:layout_constraintLeft_toLeftOf="parent"
          app:layout_constraintRight_toRightOf="parent"
          app:layout_constraintTop_toTopOf="parent" />-->

</LinearLayout>
